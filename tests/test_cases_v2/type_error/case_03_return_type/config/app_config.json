{
  "_version": "2.1.0",
  "app_name": "MyApplication",
  "database": {
    "host": "localhost",
    "port": 5432,
    "name": "myapp_db",
    "user": "admin",
    "pool_size": 10
  },
  "cache": {
    "enabled": true,
    "ttl": 3600,
    "backend": "redis",
    "redis_url": "redis://localhost:6379"
  },
  "logging": {
    "level": "INFO",
    "format": "json",
    "output": "file",
    "file_path": "/var/log/myapp.log"
  },
  "api": {
    "host": "0.0.0.0",
    "port": 8080,
    "workers": 4,
    "timeout": 30
  },
  "features": {
    "enable_metrics": true,
    "enable_tracing": false,
    "enable_profiling": false
  }
}
