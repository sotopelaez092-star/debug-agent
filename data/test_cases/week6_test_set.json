      "id": 1,
      "name": "NameError - 变量拼写错误",
      "category": "单文件",
      "error_type": "NameError",
      "difficulty": "简单",
      "project_files": {
        "main.py": "def greet(name):\n    print(f'Hello, {nme}')\n\ngreet('Tom')"
